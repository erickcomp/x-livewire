{
    "name": "erickcomp/x-livewire",
    "type": "package",
    "description": "Provides Blade Components features to Livewire components.",
    "keywords": ["laravel", "framework", "livewire"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ErickComp\\XLivewire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ErickComp\\XLivewire\\Providers\\XLivewireServiceProvider"
            ],
            "dont-discover": [
            ]
        }
    }
}
